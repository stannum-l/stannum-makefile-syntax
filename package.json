{
  "name": "stannum-makefile-syntax",
  "displayName": "Stannum Makefile Syntax",
  "description": "Makefile Syntax Highlight",
  "author": "Tin Lam <dev@chronospheres.com>",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "icon": "asset/icon.png",
  "publisher": "stannum",
  "engines": {
    "vscode": "^1.107.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stannum-l/stannum-makefile-syntax"
  },
  "bugs": {
    "url": "https://github.com/stannum-l/stannum-makefile-syntax/issues"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "makefile",
        "path": "./snippets/snippets.json"
      }
    ],
    "languages": [
      {
        "id": "Makefile",
        "aliases": [
          "makefile"
        ],
        "extensions": [
          ""
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "Makefile",
        "scopeName": "source.makefile",
        "path": "./syntaxes/Makefile.tmLanguage.json"
      }
    ]
  }
}
